@model DogModel

@if(Model == null)
{
    <h1>Dog not found</h1>
}
else
{
    <img src="~/images/@Model.Image" style="width:200px"/>
    <div><label>Name</label><span>@Model.Name</span></div>
    <div><label>Favorite Food</label><span>@Model.FavFood</span></div>
       
}


<a asp-controller="Dogs" asp-action="Index">Back to all dogs</a>